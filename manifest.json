{
    "manifest_version": 3,
    "name": "CopyPaste",
    "description": "Tap on element which you decide to copy",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["getData.js"]
      }
    ],
    "permissions": [
        "unlimitedStorage"
      ]
  }